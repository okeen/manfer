- content_for :title do t(".title") end

- comunity_services_list = ["com_fact_monthly","com_at_breakdown","comsubmit_form_contact_rep_semiannual_economy",
"com_cel_assembly","com_ad_legal","com_claim_defaulter","com_custom_attention",
"com_ask_subvention","com_budgets","com_maintenance"]

- insurances_services_list = ["ins_comunity","ins_home","ins_life","ins_car",
"ins_boat","ins_decennial","ins_house","ins_burial"]

.container
  .row
    .span8
      .well
        %h2= t(".comunity.title_comunity")
        %ul
        -comunity_services_list.each do |service|
          %li= comunity_services_traductor(service)

    .span4
      .well.pagination-centered
        %h2.label.label-important= t(".quote.title").upcase
        %p=  t ".quote.content"
        = link_to t(".quote.submit"),company_contact_path , class: "btn btn-large"
        %hr
        = image_tag "agreement.jpeg"
  .row
    .span4
      .well
        %h2= t(".insurance.title")
        %ul
        -insurances_services_list.each do |service|
          %li= insurances_services_traductor(service)

    .span8
      .well
        %h2= t(".insurance.company_work")
        %br
        = image_tag "reale.jpg"
        = image_tag "allianz.jpg"
        = image_tag "axa.jpg"
        = image_tag "caser.jpg"
        = image_tag "estrella.jpg"
        %br
        %br
        = image_tag "groupama.jpg"
        = image_tag "mapfre.jpg"
        = image_tag "mutua.jpg"
        = image_tag "vitalicio.jpg"