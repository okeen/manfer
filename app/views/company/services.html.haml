.container
  .row
    .span12
      %h1= t(".title")
  .row
    .span12
      .well
        %h1= t(".comunity.title_comunity")
        %ul
        -comunity_services_list.each do |service|
          %li= comunity_services_traductor(service)

  .row
    .span4
      .well
        %h1= t(".insurance.title")
        %ul
        -insurances_services_list.each do |service|
          %li= insurances_services_traductor(service)

